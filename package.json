{
    "name": "ripieno",
    "version": "0.0.1",
    "description": "A music notation app for web and iOS",
    "main": "./src/main.ts",
    "dependencies": {
        "audio5": "0.1.9",
        "body-parser": "~1.6.2",
        "diff": "~1.0.8",
        "envify": "~1.2.1",
        "es6-promise": ">= 1.0.0",
        "es5-shim": ">= 4.0.3",
        "express": "~4.8.2",
        "lodash": "~2.4.1",
        "midi": "http://nettek.ca/midi-0.3.0.tgz",
        "node-jsx": "~0.11.0",
        "node-lessify": "~0.0.3",
        "react": "~0.11.1",
        "react-bootstrap": "~0.11.1",
        "react-draggable": "git://github.com/mzabriskie/react-draggable",
        "react-router-component": "~0.20.1",
        "react-tools": "~0.11.1",
        "react-typescript": "git://github.com/jnetterf/react-typescript",
        "soundManager": "git://github.com/scottschiller/SoundManager2.git",
        "statics": "~0.1.0"
    },
    "devDependencies": {
        "browserify": "~5.12.1",
        "browserify-shader": "http://nettek.ca/browserify-shader.tar.gz",
        "gulp": "~3.8.8",
        "gulp-mocha": "~1.1.0",
        "gulp-uglify": "~1.0.1",
        "gulp-streamify": "~0.0.5",
        "gulp-util": "~3.0.1",
        "gulp-mocha-browserify-sweet": "~0.2.1",
        "gulp-jasmine": "~1.0.1",
        "gulp-karma": "git://github.com/jimthedev/gulp-karma.git",
        "gulp-concat": "*",
        "jisonify": "git://github.com/jnetterf/jisonify.git",
        "karma-mocha": "~0.1.9",
        "karma-phantomjs-launcher": "~0.1.4",
        "karma-coverage": "~0.2.6",
        "karma-mocha-reporter": "^0.3.0",
        "karma-notify-reporter": "~0.1.1",
        "react-typescript-definitions": "git://github.com/jnetterf/react-typescript-definitions",
        "reactify": "~0.14.0",
        "typescriptifier": "^1.0.0",
        "uglify-js": "~2.4.14",
        "vinyl-buffer": "~1.0.0",
        "vinyl-source-stream": "~1.0.0",
        "watchify": "~1.0.6"
    },
    "scripts": {
        "dev-server": "gulp watch",
        "lrc-watchify": "watchify --extension .ts -v -o build/libripienoclientBridge-bundle.js -d ./src/libripienoclientBridge.js --noparse lodash",
        "build": "NODE_ENV=production gulp build",
        "test": "gulp test",
        "test-watch": "mochify --extensions ts --watch --node $(find ./src | grep 'test/' | grep .ts$)",
        "chore": "NODE_ENV=production browserify --extension .ts --bare ./src/choreServer.ts --noparse lodash --noparse ./src/util/startExpressServer.js | uglifyjs -cm 2>/dev/null | node",
        "chore-debug": "browserify --extension .ts --bare ./src/choreServer.ts --noparse lodash --noparse ./src/util/startExpressServer.js | node"
    },
    "author": "Josh Netterfield",
    "license": "(C) Josh Netterfield 2014. All rights reserved.",
    "browserify": {
        "transform": [
            ["typescriptifier", {"extension": [".ts"]}],
            ["reactify", {"es6": true}],
            "node-lessify",
            "jisonify",
            ["browserify-shader", {"extensions": ["vs", "fs"]}]
        ]
    }
}
